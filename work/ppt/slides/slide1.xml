<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<p:sld xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main" xmlns:r="http://schemas.openxmlformats.org/officeDocument/2006/relationships" xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main">
  <p:cSld>
    <p:bg>
      <p:bgPr>
        <a:gradFill rotWithShape="1">
          <a:gsLst>
            <a:gs pos="0"><a:srgbClr val="0D1B2A"/></a:gs>
            <a:gs pos="50000"><a:srgbClr val="1B263B"/></a:gs>
            <a:gs pos="100000"><a:srgbClr val="0D1B2A"/></a:gs>
          </a:gsLst>
          <a:lin ang="5400000" scaled="0"/>
        </a:gradFill>
        <a:effectLst/>
      </p:bgPr>
    </p:bg>
    <p:spTree>
      <p:nvGrpSpPr>
        <p:cNvPr id="1" name=""/>
        <p:cNvGrpSpPr/>
        <p:nvPr/>
      </p:nvGrpSpPr>
      <p:grpSpPr>
        <a:xfrm>
          <a:off x="0" y="0"/>
          <a:ext cx="0" cy="0"/>
          <a:chOff x="0" y="0"/>
          <a:chExt cx="0" cy="0"/>
        </a:xfrm>
      </p:grpSpPr>
      <!-- Decorative gradient bar at top -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="2" name="TopBar"/>
          <p:cNvSpPr/>
          <p:nvPr/>
        </p:nvSpPr>
        <p:spPr>
          <a:xfrm>
            <a:off x="0" y="0"/>
            <a:ext cx="12192000" cy="91440"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
          <a:gradFill>
            <a:gsLst>
              <a:gs pos="0"><a:srgbClr val="00D4FF"/></a:gs>
              <a:gs pos="50000"><a:srgbClr val="7B2CBF"/></a:gs>
              <a:gs pos="100000"><a:srgbClr val="E63946"/></a:gs>
            </a:gsLst>
            <a:lin ang="0" scaled="0"/>
          </a:gradFill>
          <a:ln w="0"><a:noFill/></a:ln>
        </p:spPr>
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p><a:endParaRPr lang="en-US"/></a:p>
        </p:txBody>
      </p:sp>
      <!-- Logo/Icon placeholder -->
      <p:pic>
        <p:nvPicPr>
          <p:cNvPr id="3" name="Logo"/>
          <p:cNvPicPr><a:picLocks noChangeAspect="1"/></p:cNvPicPr>
          <p:nvPr/>
        </p:nvPicPr>
        <p:blipFill>
          <a:blip r:embed="rId2"/>
          <a:stretch><a:fillRect/></a:stretch>
        </p:blipFill>
        <p:spPr>
          <a:xfrm>
            <a:off x="5196000" y="1500000"/>
            <a:ext cx="1800000" cy="1800000"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
        </p:spPr>
      </p:pic>
      <!-- Title -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="4" name="Title"/>
          <p:cNvSpPr txBox="1"/>
          <p:nvPr/>
        </p:nvSpPr>
        <p:spPr>
          <a:xfrm>
            <a:off x="609600" y="3400000"/>
            <a:ext cx="10972800" cy="914400"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
          <a:noFill/>
        </p:spPr>
        <p:txBody>
          <a:bodyPr anchor="ctr"/>
          <a:lstStyle/>
          <a:p>
            <a:pPr algn="ctr"/>
            <a:r>
              <a:rPr lang="en-US" sz="6000" b="1">
                <a:solidFill><a:srgbClr val="00D4FF"/></a:solidFill>
                <a:latin typeface="Segoe UI Light"/>
              </a:rPr>
              <a:t>Claude</a:t>
            </a:r>
            <a:r>
              <a:rPr lang="en-US" sz="6000" b="1">
                <a:solidFill><a:srgbClr val="FFFFFF"/></a:solidFill>
                <a:latin typeface="Segoe UI Light"/>
              </a:rPr>
              <a:t>Slide</a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      <!-- Subtitle -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="5" name="Subtitle"/>
          <p:cNvSpPr txBox="1"/>
          <p:nvPr/>
        </p:nvSpPr>
        <p:spPr>
          <a:xfrm>
            <a:off x="609600" y="4400000"/>
            <a:ext cx="10972800" cy="600000"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
          <a:noFill/>
        </p:spPr>
        <p:txBody>
          <a:bodyPr anchor="ctr"/>
          <a:lstStyle/>
          <a:p>
            <a:pPr algn="ctr"/>
            <a:r>
              <a:rPr lang="en-US" sz="2400">
                <a:solidFill><a:srgbClr val="E0E1DD"/></a:solidFill>
                <a:latin typeface="Segoe UI"/>
              </a:rPr>
              <a:t>AI-Powered PowerPoint Editing with Claude's XML Understanding</a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      <!-- Tagline -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="6" name="Tagline"/>
          <p:cNvSpPr txBox="1"/>
          <p:nvPr/>
        </p:nvSpPr>
        <p:spPr>
          <a:xfrm>
            <a:off x="609600" y="5200000"/>
            <a:ext cx="10972800" cy="400000"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
          <a:noFill/>
        </p:spPr>
        <p:txBody>
          <a:bodyPr anchor="ctr"/>
          <a:lstStyle/>
          <a:p>
            <a:pPr algn="ctr"/>
            <a:r>
              <a:rPr lang="en-US" sz="1600" i="1">
                <a:solidFill><a:srgbClr val="7B2CBF"/></a:solidFill>
                <a:latin typeface="Segoe UI"/>
              </a:rPr>
              <a:t>Edit existing templates with natural language commands</a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      <!-- Bottom gradient bar -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="7" name="BottomBar"/>
          <p:cNvSpPr/>
          <p:nvPr/>
        </p:nvSpPr>
        <p:spPr>
          <a:xfrm>
            <a:off x="0" y="6766560"/>
            <a:ext cx="12192000" cy="91440"/>
          </a:xfrm>
          <a:prstGeom prst="rect"><a:avLst/></a:prstGeom>
          <a:gradFill>
            <a:gsLst>
              <a:gs pos="0"><a:srgbClr val="E63946"/></a:gs>
              <a:gs pos="50000"><a:srgbClr val="7B2CBF"/></a:gs>
              <a:gs pos="100000"><a:srgbClr val="00D4FF"/></a:gs>
            </a:gsLst>
            <a:lin ang="0" scaled="0"/>
          </a:gradFill>
          <a:ln w="0"><a:noFill/></a:ln>
        </p:spPr>
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p><a:endParaRPr lang="en-US"/></a:p>
        </p:txBody>
      </p:sp>
    </p:spTree>
  </p:cSld>
  <p:clrMapOvr><a:masterClrMapping/></p:clrMapOvr>
</p:sld>
